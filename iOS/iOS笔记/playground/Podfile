  pod 'SxrModuleManager', git: 'https://github.com/jiazifa/SxrModuleManager.git'

  pod 'Kingfisher'
  
  pod 'NVActivityIndicatorView', '~> 4.0.1' # 加载指示器
  
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  
  pod 'SwiftLint'

  pod 'Cartography', '~> 3.0'
  
end
