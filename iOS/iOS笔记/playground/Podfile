# platform :ios, '8.1'

target 'playground' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!#不显示第三方库中的警告
  
  
  pod 'SSKitSwift', git: 'https://github.com/jiazifa/SSKitSwift'
  
